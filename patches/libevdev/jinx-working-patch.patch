+#define FF_WAVEFORM_MIN	FF_SQUARE
+#define FF_WAVEFORM_MAX	FF_CUSTOM
+#define FF_GAIN		0x60
+#define FF_AUTOCENTER	0x61
+#define FF_MAX_EFFECTS	FF_GAIN
+#define FF_MAX		0x7f
+#define FF_CNT		(FF_MAX+1)
+
